{
  "name": "utils",
  "version": "0.1.0",
  "description": "Utils for 'dandanplay-resource-service' workspace",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
